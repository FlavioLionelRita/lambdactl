{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "cli version",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/cli.ts",
			"args": [
				"version"
			]
		},
		{
			"name": "cli init",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/cli.ts",
			"args": [
				"init",
				"-w",
				"lab-countries"
			]
		},
		{
			"name": "cli update",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/cli.ts",
			"args": [
				"update",
				"-w",
				"lab-countries"
			]
		},
		{
			"name": "cli sync",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/cli.ts",
			"args": [
				"sync",
				"-w",
				"lab-countries"
			]
		},
		{
			"name": "cli run bulkInsert",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/cli.ts",
			"args": [
				"run",
				"-w",
				"lab-countries",
				"-e",
				"Countries.bulkInsert()",
				"-d",
				"./lab-countries/countries.json"
				//"[{ \"\"id\"\": 4, \"\"name\"\": \"\"Afghanistan2\"\", \"\"alpha2\"\": \"\"af\"\", \"\"alpha3\"\": \"\"afg\"\"}]"
			]
		},
		{
			"name": "cli export",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/cli.ts",
			"args": [
				"export",
				"-w",
				"lab-countries",
				"-t",
				"./lab-countries/data"
			]
		},
		{
			"name": "cli run delete",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/cli.ts",
			"args": [
				"run",
				"-w",
				"lab-countries",
				"-e",
				"Countries.deleteAll()"
			]
		},
		{
			"name": "cli import",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/cli.ts",
			"args": [
				"import",
				"-w",
				"lab-countries",
				"-s",
				"./lab-countries/data/mydb-export.json"
			]
		},
		{
			"name": "cli run query",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/cli.ts",
			"args": [
				"run",
				"-w",
				"lab-countries",
				"-n",
				"mydb",
				"-e",
				"Countries.filter( p=> p.alpha2 == 'br')"
			]
		},
		{
			"name": "cli run query with context",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/cli.ts",
			"args": [
				"run",
				"-w",
				"lab-countries",
				"-e",
				"OrderDetails.filter(p => between(p.order.shippedDate, from, to) && p.unitPrice > minValue).map(p => ({ category: p.product.category.name, product: p.product.name, unitPrice: p.unitPrice, quantity: p.quantity })).sort(p => [p.category, p.product])",
				"-d",
				//"{ \"\"a\"\":\"\"1997-01-01\"\" }"
				"{ \"\"minValue\"\": 10, \"\"from\"\": \"\"1997-01-01\"\", \"\"to\"\": \"\"1997-12-31\"\" }"
				//"[{ \"\"id\"\": 4, \"\"name\"\": \"\"Afghanistan2\"\", \"\"alpha2\"\": \"\"af\"\", \"\"alpha3\"\": \"\"afg\"\"}]"
			]
		},
		{
			"name": "cli drop",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/cli.ts",
			"args": [
				"drop",
				"-w",
				"lab-countries"
			]
		}
	]
}