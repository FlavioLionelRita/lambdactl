{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "version",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"version"
			]
		},
		// counties -------------------------------------------------------------------------------------------------------------------------------------
		{
			"name": "countries init",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"init",
				"-w",
				"lab/countries"
			]
		},
		{
			"name": "countries update",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"update",
				"-w",
				"lab/countries"
			]
		},
		{
			"name": "countries sync",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"sync",
				"-w",
				"lab/countries"
			]
		},
		{
			"name": "countries execute bulkInsert",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"execute",
				"-w",
				"lab/countries",
				"-q",
				"Countries.bulkInsert().include(p => p.states)",
				"-d",
				"./lab/countries/countries.json"
			]
		},
		{
			"name": "countries export",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"export",
				"-w",
				"lab/countries",
				"-t",
				"./lab/countries/data"
			]
		},
		{
			"name": "countries delete States",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"execute",
				"-w",
				"lab/countries",
				"-q",
				"States.deleteAll()"
			]
		},
		{
			"name": "countries delete Countries",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"execute",
				"-w",
				"lab/countries",
				"-q",
				"Countries.deleteAll()"
			]
		},
		{
			"name": "countries import",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"import",
				"-w",
				"lab/countries",
				"-d",
				"./lab/countries/data/default-export.json"
			]
		},
		{
			"name": "countries execute query",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"execute",
				"-w",
				"lab/countries",
				"-q",
				"Countries.filter( p=> p.iso3 == 'AFG' || p.iso3 == 'ARG'  ).include(p=> p.states)"
			]
		},
		{
			"name": "countries drop",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"drop",
				"-w",
				"lab/countries"
			]
		},
		// devicenet-----------------------------------------------------------------------------------------------
		{
			"name": "devicenet init",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"init",
				"-w",
				"lab/devicenet"
			]
		},
		{
			"name": "devicenet update",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"update",
				"-w",
				"lab/devicenet"
			]
		},
		{
			"name": "devicenet sync",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"sync",
				"-w",
				"lab/devicenet",
				"-e",
				"lab/devicenet/.env"
			]
		},
		{
			"name": "devicenet get sentence",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"execute",
				"-w",
				"lab/devicenet",
				"-e",
				"lab/devicenet/.env",
				"-q",
				"Devices.bulkInsert().include(p => p.components)",
				"-o",
				"sentence"
			]
		},
		{
			"name": "devicenet get constraints",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"execute",
				"-w",
				"lab/devicenet",
				"-e",
				"lab/devicenet/.env",
				"-q",
				"Devices.bulkInsert().include(p => p.components)",
				"-o",
				"constraints"
			]
		},
		{
			"name": "devicenet drop",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"drop",
				"-w",
				"lab/devicenet",
				"-e",
				"lab/devicenet/.env"
			]
		}
	]
}