				"export",
				"-w",
				"lab/countries",
				"-t",
				"./lab/countries/data",
				"-e",
				"lab/countries/.env"
			]
		},
		{
			"name": "countries delete States",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
