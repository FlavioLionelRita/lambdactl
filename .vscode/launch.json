{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "cli version",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"version"
			]
		},
		{
			"name": "cli init",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"init",
				"-w",
				"lab-countries"
			]
		},
		{
			"name": "cli update",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"update",
				"-w",
				"lab-countries"
			]
		},
		{
			"name": "cli sync",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"sync",
				"-w",
				"lab-countries"
			]
		},
		{
			"name": "cli run",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"run",
				"-w",
				"lab-countries",
				"-e",
				"Countries.bulkInsert()",
				"-c",
				"./lab-countries/countries.json"
				//"[{ \"\"id\"\": 4, \"\"name\"\": \"\"Afghanistan2\"\", \"\"alpha2\"\": \"\"af\"\", \"\"alpha3\"\": \"\"afg\"\"}]"
			]
		},
		{
			"name": "cli export",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"export",
				"-w",
				"lab-countries",
				"-t",
				"./lab-countries/data"
			]
		},
		{
			"name": "cli run delete",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"run",
				"-w",
				"lab-countries",
				"-e",
				"Countries.deleteAll()"
			]
		},
		{
			"name": "cli import",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"import",
				"-w",
				"lab-countries",
				"-s",
				"./lab-countries/data/mydb-export.json"
			]
		},
		{
			"name": "cli run query",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"run",
				"-w",
				"lab-countries",
				"-n",
				"mydb",
				"-e",
				"Countries.filter( p=> p.alpha2 == 'br')"
			]
		},
		{
			"name": "cli drop",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"drop",
				"-w",
				"lab-countries"
			]
		}
	]
}