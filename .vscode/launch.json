			"name": "countries delete Countries",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"execute",
				"-w",
				"lab/countries",
				"-q",
				"Countries.deleteAll()",
				"-e",
				"lab/countries/.env"
			]
		},
