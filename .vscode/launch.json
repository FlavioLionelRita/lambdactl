			"name": "countries import",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"import",
				"-w",
				"lab/countries",
				"-d",
				"./lab/countries/data/default-export.json",
				"-e",
				"lab/countries/.env"
			]
		},
		{
			"name": "countries execute query",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"execute",
				"-w",
				"lab/countries",
				"-q",
				"Countries.filter(p=>p.iso3=='AFG'||p.iso3=='ARG').include(p=> p.states)",
				"-e",
				"lab/countries/.env"
			]
		},
		{
			"name": "countries drop",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"args": [
				"drop",
				"-w",
				"lab/countries",
				"-e",
				"lab/countries/.env"
			]
		},
