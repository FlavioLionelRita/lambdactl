{
	"eslint.format.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
	"editor.tabSize": 2,
	"editor.insertSpaces": false,
	"cSpell.words": [
		"commitlint",
		"concat",
		"datetime",
		"Datetime",
		"devicenet",
		"ecma",
		"envfile",
		"firstname",
		"Flavio",
		"github",
		"gruntfile",
		"imei",
		"initcap",
		"lambdaorm",
		"lastname",
		"lpad",
		"ltrim",
		"MYSQL",
		"northwind",
		"npmjs",
		"nullable",
		"oracledb",
		"rpad",
		"rtrim",
		"sqlite"
	],
	"editor.inlineSuggest.showToolbar": "onHover"
}